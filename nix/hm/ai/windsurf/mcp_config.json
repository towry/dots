{
  "mcpServers": {
    "kg": {
      "serverUrl": "@KG_SSE@",
      "headers": {
        "Authorization": "Bearer @KG_API_KEY@"
      }
    },
    "context7": {
      "serverUrl": "https://mcp.context7.com/mcp"
    },
    "brightdata": {
      "command": "bunx",
      "args": [
        "@brightdata/mcp"
      ],
      "env": {
        "API_TOKEN": "@BRIGHTDATA_API_KEY@"
      },
      "disabledTools": []
    },
    "playwright": {
      "command": "bunx",
      "args": [
        "@playwright/mcp@latest",
        "--executable-path",
        "google-chrome-stable",
        "--extension"
      ],
      "disabledTools": [],
      "disabled": false
    },
    "github": {
      "command": "github-mcp-server",
      "args": [
        "stdio",
        "--dynamic-toolsets"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "@GITHUB_PAT@"
      },
      "disabledTools": []
    }
  }
}
