{
  "mcpServers": {
    "chromedev": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--browser-url=http://127.0.0.1:9222"
      ],
      "disabled": false
    },
    "github": {
      "type": "stdio",
      "command": "github-mcp-server",
      "args": ["stdio", "--dynamic-toolsets"],
      "disabled": false,
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "@GITHUB_PERSONAL_ACCESS_TOKEN@"
      }
    },
    "kg": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "mcp-remote",
        "@KG_SSE@",
        "--allow-http",
        "--header",
        "Authorization: Bearer @KG_API_KEY@"
      ]
    },
    "mastergo": {
      "type": "stdio",
      "args": [
        "@mastergo/magic-mcp",
        "--token",
        "@MASTERGO_API_KEY@",
        "--url=https://mastergo.com"
      ],
      "disabled": true,
      "command": "bunx",
      "env": {}
    },
    "brightdata": {
      "type": "stdio",
      "command": "bunx",
      "args": ["@brightdata/mcp@2.6.1"],
      "disabled": false,
      "env": {
        "API_TOKEN": "@BRIGHTDATA_API_TOKEN@"
      }
    }
  }
}
