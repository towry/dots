  },
  "enableCompletionBell": true,
  "completionSound": "fx-ok01",
  "awaitingInputSound": "fx-ack01",
  "soundFocusMode": "always",
  "commandAllowlist": [
    "mix format",
    "ls",
    "pwd",
    "dir",
    "cd",
    "cat",
    "echo",
    "head",
    "tail",
    "curl",
    "grep",
    "find",
    "glob",
    "rg",
    "fd",
    "ast-grep",
    "git show",
    "git diff",
    "git status",
    "git log",
    "git --no-pager",
    "mkdir",
    "jj git-diff",
    "pnpm turbo run build:dev",
    "lsof",
    "ps",
    "cd ~/workspace/llm/research/ && sgrep index",
    "cd ~/workspace/llm/research/ && sgrep search:*",
    "bash /Users/towry/.claude/skills/git-jj/scripts/repo_check.sh",
    "bash ~/.claude/skills/git-jj/scripts/repo_check.sh",
    "bash ~/.claude/skills/fast-repo-context/scripts/sgrep.sh:*",
    "~/.claude/skills/fast-repo-context/scripts/sgrep.sh:*",
    "python3 ~/.claude/skills/local-research/scripts/research_ops.py list:*",
    "bunx repomix:*",
    "jj --no-pager:*",
    "jj diff:*",
    "jj new:*",
    "jj log:*",
    "jj show:*",
    "jj status:*",
    "jj file:*",
    "cargo clippy:*",
    "cargo test:*",
    "cargo check:*",
    "terminal-notifier"
