  "commandDenylist": [
    "rm -rf /",
    "rm -rf /*",
    "rm -rf .",
    "rm -rf ~",
    "rm -rf ~/*",
    "rm -rf $HOME",
    "rm -r /",
    "rm -r /*",
    "rm -r ~",
    "rm -r ~/*",
    "mkfs",
    "mkfs.ext4",
    "mkfs.ext3",
    "mkfs.vfat",
    "mkfs.ntfs",
    "dd if=/dev/zero of=/dev",
    "dd of=/dev",
    "shutdown",
    "reboot",
    "halt",
    "poweroff",
    "init 0",
    "init 6",
    ":(){ :|: & };:",
    ":() { :|:& };:",
    "chmod -R 777 /",
    "chmod -R 000 /",
    "chown -R",
    "format",
    "powershell Remove-Item -Recurse -Force"
  ],
  "enableCustomDroids": true,
  "enableHooks": true,
  "includeCoAuthoredByDroid": true,
  "enableDroidShield": true,
  "enableReadinessReport": false,
  "todoDisplayMode": "pinned",
  "autonomyLevel": "normal",
  "specSaveEnabled": true,
  "specModeModel": "claude-opus-4-5-20251101",
  "specModeReasoningEffort": "high",
  "allowBackgroundProcesses": true,
  "claudeHooksImported": false,
  "importedClaudeHooks": [
    "uv run ~/.claude/hooks/pre_compact.py",
    "terminal-notifier -sound default -message 'Claude Code: Awaiting your input'"
  ],
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.dotfiles/nix/hm/ai/claude/hooks/session_remind.py"
          }
        ]
      },
      {
        "matcher": "clear",
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.dotfiles/nix/hm/ai/claude/hooks/session_start_handoff.py"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.dotfiles/nix/hm/ai/claude/hooks/handoff_interceptor.py"
          },
          {
            "type": "command",
            "command": "uv run ~/.dotfiles/nix/hm/ai/claude/hooks/pickup_hook.py"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Execute",
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.dotfiles/nix/hm/ai/claude/hooks/prevent_forbidden_bash.py"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "SlashCommand",
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.dotfiles/nix/hm/ai/claude/hooks/post_pickup_handler.py"
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.claude/hooks/session_save.py"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": []
      }
    ],
    "Notification": []
  },
  "hooksDisabled": true
}
